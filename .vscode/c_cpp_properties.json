{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceRoot}",
                "${workspaceRoot}/modules/upnp/inc",
                "${workspaceRoot}/modules/upnp/src",
                "${workspaceRoot}/modules/utils/inc",
                "${workspaceRoot}/modules/utils/src",
                "${workspaceRoot}/modules/audio/inc",
                "${workspaceRoot}/modules/audio/src",
                "${workspaceRoot}/build/local-native/include",
                "${workspaceRoot}/modules/image/inc",
                "${workspaceRoot}/modules/image/src",
                "/usr/include",
                "/usr/local/include",
                "/usr/local/include/c++/v1",
                "/usr/include/machine"
            ],
            "intelliSenseMode": "clang-x64",
            "macFrameworkPath": [
                "/System/Library/Frameworks",
                "/Library/Frameworks"
            ],
            "compileCommands": "${workspaceRoot}/build/doozy-clang"
        },
        {
            "name": "Linux",
            "includePath": [
                "/usr/include",
                "${workspaceRoot}"
            ],
            "intelliSenseMode": "clang-x64",
            "browse": {
                "path": [
                    "/usr/include",
                    "${workspaceRoot}"
                ]
            }
        },
        {
            "name": "Win32",
            "includePath": [
                "c:/Program Files (x86)/Microsoft Visual Studio 14.0/VC/include",
                "${workspaceRoot}"
            ],
            "intelliSenseMode": "msvc-x64",
            "browse": {
                "path": [
                    "c:/Program Files (x86)/Microsoft Visual Studio 14.0/VC/include",
                    "${workspaceRoot}"
                ]
            }
        }
    ],
    "clang_format": {
        "style": "file",
        "fallback-style": "LLVM",
        "sort-includes": false
    },
    "version": 3
}